[alias]
	up = fetch -p
	last = log -1 HEAD --stat
	l = remote -v
	b = branch -a
	c = commit -m
	s = status -sb
	stat = status
	sub = submodule update --recursive --remote
	aliases = config --get-regex ^alias

[init]
	defaultBranch = master

[core]
	autocrlf = false
	filemode = false
	sshCommand = ssh.exe

[includeIf "gitdir:**/.config/**/.git"]
  path = github.gitconfig